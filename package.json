{
  "name": "jsDemo",
  "version": "1.0.0",
  "description": "demo project",
  "main": "index.js",
  "author": "Kostya Podlesny",
  "license": "MIT",
  "scripts": {
    "start": "rimraf ./dist && babel -d ./dist ./src -s && node ./dist/main.js && yarn run db-create",
    "dev": "node dev.js",
    "lint:js": "eslint ./src",
    "lint-staged": "lint-staged",
    "db-create": "node_modules/.bin/sequelize db:create migration_development && node_modules/.bin/sequelize db:migrate"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "lint-staged": "^7.2.2",
    "pre-commit": "^1.2.2",
    "check-node-version": "10.0.0"
  },
  "engines": {
    "node": "10.0.0"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "rules": {
      "import/no-dynamic-require": 0
    },
    "env": {
      "node": true
    }
  },
  "pre-commit": "lint-staged",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "pg": "^7.4.3",
    "rimraf": "^2.6.2",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.1.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
